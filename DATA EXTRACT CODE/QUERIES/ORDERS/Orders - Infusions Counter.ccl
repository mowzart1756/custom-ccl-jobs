SELECT
	cnt_distinct = count(distinct o.order_id)

FROM
	ORDERS O
	, ORDER_DETAIL O_D

PLAN O_D
	WHERE
	 O_D.OE_FIELD_VALUE = 318173.00 ;"IV Infusion"
	AND O_D.UPDT_DT_TM > CNVTDATETIME("01-JAN-2023 00:00:00.00")

JOIN O
	WHERE O_D.ORDER_ID = O.ORDER_ID
 	AND O.ORIG_ORDER_DT_TM > CNVTDATETIME("01-JAN-2023")
 	AND CNVTUPPER(O.ORDER_MNEMONIC) = "*IRON*"

WITH TIME = 1000
;2047