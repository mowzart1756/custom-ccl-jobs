SELECT
	 C.RESULT_VAL
	, C_RESULT_UNITS_DISP = UAR_GET_CODE_DISPLAY(C.RESULT_UNITS_CD)
	, C.RESULT_STATUS_CD
	, C_RESULT_STATUS_DISP = UAR_GET_CODE_DISPLAY(C.RESULT_STATUS_CD)
	, C.UPDT_DT_TM
	, UR_NUMBER = P.ALIAS

FROM
	CLINICAL_EVENT   C
	, (LEFT JOIN PERSON_ALIAS P ON (P.PERSON_ID = C.PERSON_ID))

PLAN C
	WHERE 
		C.UPDT_DT_TM > CNVTLOOKBEHIND("10,D")
		AND
		C.contributor_system_cd = 86524974 ; Western Health Pathology
		AND
		C.RESULT_STATUS_CD = 30.00 ; STATUS CODE: In Error - Delete
		
JOIN P
	WHERE
		P.ALIAS_POOL_CD = 9569589 ; this filters for the UR Number

WITH MAXREC = 1000, NOCOUNTER, SEPARATOR=" ", FORMAT, TIME = 1200