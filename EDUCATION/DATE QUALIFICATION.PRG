SELECT
	P.BEG_EFFECTIVE_DT_TM
	, P.USERNAME
	, DATE = P.BEG_EFFECTIVE_DT_TM "YYYY-DD-MM HH:MM:SS;;D"

FROM
	PRSNL   P
;DATE QUALIFICATION BELOW
WHERE 
	P.BEG_EFFECTIVE_DT_TM >= CNVTDATETIME("31-DEC-2014 23:59:59.00")
	AND
	P.BEG_EFFECTIVE_DT_TM <= CNVTDATETIME("31-DEC-2015 23:59:59.00")

WITH MAXREC = 100, NOCOUNTER, SEPARATOR=" ", FORMAT