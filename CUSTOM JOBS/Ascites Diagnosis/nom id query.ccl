SELECT DISTINCT
	D.DIAGNOSIS_DISPLAY
	, D.NOMENCLATURE_ID

FROM
	DIAGNOSIS   D

WHERE
	D.DIAGNOSIS_DISPLAY = "*ascites*"
	OR
	D.DIAGNOSIS_DISPLAY = "*Ascites*"
	OR
	D.DIAGNOSIS_DISPLAY = "*ASCITES*"
	OR
	D.DIAGNOSIS_DISPLAY = "*ascite*"

ORDER BY
	D.NOMENCLATURE_ID
	, 0

WITH MAXREC = 1000, NOCOUNTER, SEPARATOR=" ", FORMAT, TIME = 10