SELECT
      TEST = O.ORDERED_AS_MNEMONIC
      , O.ORDER_ID
FROM
    , ORDERS                O
WHERE
    O.ORDER_ID = O_A.ORDER_ID
    AND O.CATALOG_TYPE_CD = 2513 /* 'Laboratory' Orders Only */
        AND O.ORIG_ORDER_DT_TM > CNVTDATETIME("01-JAN-2019 00:00:00.00")
     AND O.PERSON_ID NOT IN ; REMOVE TEST PATIENTS
        (SELECT PERSON_FILTER.PERSON_ID FROM PERSON PERSON_FILTER WHERE PERSON_FILTER.NAME_LAST_KEY = "*TESTWHS*")
WITH TIME = 500