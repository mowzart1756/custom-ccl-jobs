SELECT E.ENCNTR_ID
WHERE
    E.ENCNTR_ID = O.ENCNTR_ID
    AND E.LOC_FACILITY_CD != 4038465.00 /* "DEMO 1 HOSPITAL" Removes Fake Data From The Demo Hospital */
    AND E.BEG_EFFECTIVE_DT_TM > CNVTDATETIME("01-JAN-2018 00:00:00.00") /* Encounters after  */
    AND E.LOC_FACILITY_CD = 86163400.00 /* Sunshine Hospital Only */
    AND E.PERSON_ID NOT IN ; REMOVE TEST PATIENTS
        (SELECT PERSON_FILTER.PERSON_ID FROM PERSON PERSON_FILTER WHERE PERSON_FILTER.NAME_LAST_KEY = "*TESTWHS*")
WITH TIME = 100