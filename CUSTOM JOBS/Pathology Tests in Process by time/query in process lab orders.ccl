SELECT
      O_A.ORDER_ID
    , DT_YEAR = O_A.EFFECTIVE_DT_TM "YYYY"
    , DT_MONTH = O_A.EFFECTIVE_DT_TM "MM"
    , DT_TIME = O_A.EFFECTIVE_DT_TM "HH:MM"

FROM
      ORDER_ACTION          O_A

WHERE
    O_A.ORDER_STATUS_CD = 2548 /* 'INPROCESS' Order Status Rows Only */
    AND O_A.ACTION_DT_TM BETWEEN
        CNVTDATETIME("01-JAN-2022 00:00:00.00")
        AND
        CNVTDATETIME("01-JAN-2023 00:00:00.00")
    AND O_A.CONTRIBUTOR_SYSTEM_CD = 86524974.00 /* 'WH_LAB' Laboratory orders only*/

WITH TIME = 500