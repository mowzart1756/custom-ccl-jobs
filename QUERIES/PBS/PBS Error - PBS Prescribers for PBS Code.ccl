SELECT
	P_P.PBS_ITEM_CODE
	, P_PRESCRIBER_TYPE_DISP = UAR_GET_CODE_DISPLAY(P_P.PRESCRIBER_TYPE_CD)
	, P_P.END_EFFECTIVE_DT_TM "YYYY-MMM-DD HH:MM:SS;;D"
	, P_P.BEG_EFFECTIVE_DT_TM "YYYY-MMM-DD HH:MM:SS;;D"
	, P_P.PBS_PRESCRIBER_ID
	, P_P.UPDT_DT_TM "YYYY-MMM-DD HH:MM:SS;;D"
	, P_P.UPDT_ID


FROM
	PBS_PRESCRIBER   P_P

WHERE
    P_P.PBS_ITEM_CODE = "12663L"

; ORDER BY
; 	P_P.PBS_ITEM_CODE
; 	, P_P.END_EFFECTIVE_DT_TM   DESC

WITH NOCOUNTER, SEPARATOR=" ", FORMAT, TIME= 29
